        xmlns:viewModels="clr-namespace:NewProjectCreatorUI.ViewModels"
        xmlns:localizations="clr-namespace:NewProjectCreatorUI.Resources.Localizations"
        mc:Ignorable="d"
        x:Class="NewProjectCreatorUI.Views.MainWindow"
